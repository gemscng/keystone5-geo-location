{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "allowUmdGlobalAccess": true,
    "paths": {
      "*": [
        "node_modules/*",
      ]
    },
    "jsx": "react"
  },
  "include": [
    "src/**/*"
  ]
}
